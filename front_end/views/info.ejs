<header class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-body border-bottom shadow-sm">
  <p class="h5 my-0 me-md-auto fw-normal"><a class="navbar-brand" href="/">E-voting</a> </p>
  <nav class="my-2 my-md-0 me-md-3">
    
    <a class="p-2 text-dark" href="/users/register">Sign in</a>
  </nav>
  
  <a class="btn btn-outline-primary" href="/users/login">Sign up</a>
</header>
<br>
<main class="form-signin">
  <%- include ("./partial/alert"); %>
  
    <h1>Vote informations</h1>
    

    <% if(typeof candidats != 'undefined') { %>
  <% if(candidats.length != 0) { %>
  
  <div class="accordion" id="accordionExample">
    <% candidats.forEach(function(candidat){ %>
      
      <label"><%= candidat.nom%> : <%= candidat.nombreDeVote %></label>
    
 
    <% }); %>
  </div>  
  <% } %>
<% } %>

  
</main>
